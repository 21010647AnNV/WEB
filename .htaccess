RewriteEngine On
RewriteBase /home.html
RewriteCond %{REQUEST_FILENAME} !-f [OR] !-d
RewriteCond %{REQUEST_FILENAME} !-s
RewriteRule ^register$ /register.php.php [L]
RewriteRule ^login$ /login.php.php [L]
RewriteRule ^about$ /about.php.php [L]
RewriteRule ^ranking$ /ranking.php.php [L]
RewriteRule ^profile$ /profile.php.php [L]
RewriteRule ^logout$ /logout.php.php [L]
RewriteRule ^forgotpassword$ /forgotpassword.php.php [L]
RewriteRule ^naptien$ /topup/index.php.php [L]
RewriteRule ^changepassword$ /changepassword.php.php [L]
RewriteRule ^([0-9]+)/([0-9]+).png$ /avatar.php?user_id=$1&nv=$2 [L]
RewriteRule ^equip$ /equip.php.php [L]
RewriteRule ^changeemail$ /changeemail.php.php [L]
RewriteRule ^addemail$ /addemail.php.php [L]
RewriteRule ^changesignature$ /changesignature.php.php [L]
RewriteRule ^addsignature$ /addsignature.php.php [L]
RewriteRule ^addpoint$ /addpoint.php.php [L]
RewriteRule ^forgotsignature$ /forgotsignature.php.php [L]
RewriteRule ^shop/dang-ban$ /shop/sell.php [L]
RewriteRule ^shop/quan-ly$ /shop/manager.php [L]
RewriteRule ^shop/$ /shop/index.php?page=1 [L]
RewriteRule ^([0-9]+)/shop/$ /shop/index.php?page=$1 [L]
RewriteRule ^shop/([0-9]+)/account$ /shop/account.php?user_id=$1 [L]
RewriteRule ^shop/([0-9]+)/edit$ /shop/edit.php?id=$1 [L]
RewriteRule ^shop/([0-9]+)/delete$ /shop/delete.php?id=$1 [L]
